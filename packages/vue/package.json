{
  "name": "@name-ui/vue",
  "version": "1.0.0",
  "description": "Vue components for Name-UI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "clean": "if exist dist rmdir /s /q dist"
  },
  "dependencies": {
    "@name-ui/core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  },
  "keywords": [
    "ui",
    "components",
    "vue",
    "vue3"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}