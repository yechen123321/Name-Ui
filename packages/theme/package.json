{
  "name": "@name-ui/theme",
  "version": "1.0.0",
  "description": "Theme and styles for Name-UI",
  "type": "module",
  "main": "./dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/index.css"
    },
    "./dark": {
      "import": "./dist/dark.css"
    }
  },
  "files": [
